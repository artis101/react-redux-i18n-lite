    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "moment": false,
      "intl": false,
      "global": false
    },
    "plugins": [
      "react"
    ],
    "extends": "airbnb",
    "rules": {
      "arrow-parens": "off"
